<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="menuPrivilege" namespace="/" extends="json-default">
		<action name="privilegeAction_*" method="{1}" class="privilegeAction">
			<result type="json"></result>
		</action>
	</package>
	<package name="ajaxPrivilege" namespace="/" extends="ajax-default">
		<action name="ajaxPrivilegeAction_*" method="{1}" class="privilegeAction">
			<result type="json"></result>
		</action>
	</package>
</struts>